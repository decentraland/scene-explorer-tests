{
  "name": "@dcl/scene-explorer-tests",
  "description": "SDK7 Template scene",
  "version": "1.0.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "glob": "^10.3.10",
    "prettier": "3.0.3",
    "syncpack": "^11.2.1",
    "testing-library": "./testing-library/"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "static"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "repository": "decentraland/scene-explorer-tests",
  "scripts": {
    "build": "sdk-commands build --skip-install",
    "deploy": "sdk-commands deploy",
    "export-static": "sdk-commands export-static",
    "format": "prettier --config .prettierrc -c ./**/*.{ts,tsx}",
    "format:fix": "prettier -w --config .prettierrc -c ./**/*.{ts,tsx}",
    "lint": "eslint ./**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./**/*.{ts,tsx}",
    "start": "sdk-commands start --skip-build -w"
  },
  "dependencies": {
    "@dcl/sdk": "^7.4.5-8054791102.commit-f06a0a7"
  }
}
